'' Euclid's algorithm ''

begin
  a := 1071...
  b := 462...
  if a < b then
    begin
      c := a...
      a := b...
      b := c
    end
  else...
  while 0 < b do
    begin
      c := b...
      while b < a+1 do     ** computation of modulo
        a := a-b...
      b := a...
      a := c
    end...
  print(a)
end